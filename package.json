    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.20",
    "bun-types": "^1.2.4",
    "cssnano": "^7.0.6",
    "daisyui": "^5.0.0",
    "eslint": "^9.11.1",
    "eslint-plugin-vue": "^10.5.1",
    "gh-pages": "^6.2.0",
