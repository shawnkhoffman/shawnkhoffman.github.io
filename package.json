{
  "name": "shawnkhoffman.dev",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --bun vite",
    "build": "bun run vite build && cp dist/index.html dist/404.html",
    "lint": "bun run eslint .",
    "test": "bun test src/tests/**/*.test.tsx --environment=jsdom",
    "test:coverage": "bun test src/tests/**/*.test.tsx --environment=jsdom --coverage",
    "test:watch": "bun test src/tests/**/*.test.tsx --environment=jsdom --watch",
    "test:ui": "bun test --ui",
    "test:specific": "bun test --environment=jsdom",
    "lighthouse": "lighthouse http://localhost:5173 --view",
    "preview": "bun run vite preview",
    "analyze": "bun run vite build --mode analyze",
    "predeploy": "bun run build",
    "deploy": "bun run gh-pages -d dist"
  },
  "dependencies": {
    "@datadog/browser-rum": "^5.29.0",
    "@happy-dom/global-registrator": "^17.2.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^7.2.0",
    "vite-plugin-sitemap": "^0.7.1",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.0",
    "@eslint/js": "^9.11.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bun": "^1.2.4",
    "@types/gtag.js": "^0.0.20",
    "@types/i18next": "^12.1.0",
    "@types/jest-axe": "^3.5.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-i18next": "^7.8.3",
    "@types/react-test-renderer": "^18.3.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "bun-types": "^1.2.4",
    "cssnano": "^7.0.6",
    "daisyui": "^4.12.13",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "gh-pages": "^6.2.0",
    "globals": "^15.9.0",
    "i18next": "^23.16.0",
    "jest-axe": "^9.0.0",
    "jsdom": "^26.0.0",
    "lighthouse": "^9.3.1",
    "postcss": "^8.4.47",
    "react-i18next": "^15.0.3",
    "react-test-renderer": "^18.3.1",
    "tailwindcss": "^3.4.13",
    "terser": "^5.36.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-plugin-compression": "^0.5.1",
