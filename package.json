  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --bun vite",
    "build": "bun run vite build && cp dist/index.html dist/404.html",
    "lint": "bun run eslint .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:specific": "vitest",
    "lighthouse": "lighthouse http://localhost:5173 --view",
    "preview": "bun run vite preview",
    "analyze": "bun run vite build --mode analyze",
    "predeploy": "bun run build",
    "deploy": "bun run gh-pages -d dist"
  },
  "dependencies": {
    "@datadog/browser-rum": "^6.4.0",
