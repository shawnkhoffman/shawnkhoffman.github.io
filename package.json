    "@iconify/vue": "^5.0.0",
    "caniuse-lite": "^1.0.30001754",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^3.0.2",
    "vite-plugin-sitemap": "^0.7.1",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@tailwindcss/postcss": "^4.0.10",
